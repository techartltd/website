    <sql>
-- 57. Creating person relatioship table
DROP TABLE IF EXISTS migration_st.st_person_relationship;
CREATE TABLE migration_st.st_person_relationship
(
	Index_Person_Id						int(11),
	Relative_Person_Id					int(11),		
	RelativeFirstName					VARCHAR(255),
	RelativeMiddleName					VARCHAR(255),
	RelativeLastName					VARCHAR(255),
	RelativeSex							VARCHAR(100),
	Relationship						VARCHAR(200),
	created_at							DATE,
	created_by							int(11),
	Voided								VARCHAR(10)
)
</sql>
