ord.Ptn_pk,
ord.OrderedByDate,
dtl_ord.Duration,pt.Id as patient_id,
dateadd(day,dtl_ord.Duration,ord.OrderedByDate) as tca
   from ord_PatientPharmacyOrder ord 
   inner join (
   SELECT CAST(Row_Number() OVER (PARTITION BY dtl_ord.ptn_pharmacy_pk ORDER
      BY dtl_ord.duration DESC) AS Varchar) AS RowID,
      dtl_ord.ptn_pharmacy_pk,
      dtl_ord.Duration
    FROM dtl_PatientPharmacyOrder dtl_ord
   ) dtl_ord on dtl_ord.ptn_pharmacy_pk=ord.ptn_pharmacy_pk and dtl_ord.RowID=1
   left join (select id,name from LookupItem) lr on lr.id=ord.RegimenLine
   left join patient pt on pt.ptn_pk=ord.Ptn_pk
	) tca on tca.patient_id=pe.PatientId and cast(pm.visitdate AS date)=cast(tca.OrderedByDate as date)
