				WHEN a.DifficultyRemembering = 0.25
					THEN 'Once in a while'
				WHEN a.DifficultyRemembering = 0.5
					THEN 'Sometimes'
				WHEN a.DifficultyRemembering = 0.75
					THEN 'Usually'
				WHEN a.DifficultyRemembering = 1
