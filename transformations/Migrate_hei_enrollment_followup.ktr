  Final_DBS_Sample_Code_Result        VARCHAR(100),
  DBS_Sample_Code							        VARCHAR(100),
  Date_DBS_Sample_Code_Result_collected	 DATE,
  Tetracycline_Eye_Ointment  	          VARCHAR(100),
  Pupil 								                VARCHAR(100),
  Sight              				  		  		VARCHAR(100),
  Squint				                        VARCHAR(100),
  Deworming_and_Vitamin_A_supp		    	VARCHAR(100),
