  Immunization_given 		    			VARCHAR(100),
  Date_immunized 				      		DATE,
  Date_of_next_appointment	  		DATE,
  Revisit_this_year 			  	    VARCHAR(100),
  Primary_care_giver			      		VARCHAR(100),
  MUAC								                VARCHAR(100),
  Exclusive_breastfeeding_0_6_Months  VARCHAR(100),
  Stunted                             VARCHAR(100),
  Medication_Given_If_HIV_Exposed     VARCHAR(100),
  TB_screening_outcome				        VARCHAR(100),
  Development_Milestones_by_age	     	VARCHAR(100),
  Achieving_milestones				        VARCHAR(100),
  Weight_category					          	VARCHAR(100),
  Type_of_follow_up				          	VARCHAR(100),
  Test_contextual_status		      		VARCHAR(100),
  Contextual_status_results	       		VARCHAR(100),
  First_DBS_sample_code				    		VARCHAR(50),
